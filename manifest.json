{
  "manifest_version": 3,
  "name": "Recent Bookmarks",
  "version": "1.0",
  "description": "Display a list of recent bookmarks",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["bookmarks"]
}
